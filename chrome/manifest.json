{
  "manifest_version": 3,
  "name": "Wheel Toggle Tab",
  "version": "1.0.0",
  "description": "Hold Alt key and scroll mouse wheel to switch between tabs",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ]
}
